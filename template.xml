<Container>
  <Name>wowchat-epoch</Name>
  <Repository>doctorbeefy/wowchat-epoch</Repository>
  <Registry>https://hub.docker.com</Registry>
  <Network>bridge</Network>
  <Icon>https://styles.redditmedia.com/t5_2qio8/styles/communityIcon_kk65kxekp2hd1.png</Icon>
  <Overview>My custom Docker image for WowChat.</Overview>
  <WebUI></WebUI>
  <ExtraParams></ExtraParams>
  <Env>
    <Variable>
      <Key>DISCORD_TOKEN</Key>
      <Default></Default>
      <Description>Discord bot token.</Description>
    </Variable>
    <Variable>
      <Key>WOW_ACCOUNT</Key>
      <Default></Default>
      <Description>World of Warcraft account name.</Description>
    </Variable>
    <Variable>
      <Key>WOW_PASSWORD</Key>
      <Default></Default>
      <Description>World of Warcraft account password.</Description>
    </Variable>
    <Variable>
      <Key>WOW_CHARACTER</Key>
      <Default></Default>
      <Description>World of Warcraft character name.</Description>
    </Variable>
    <Variable>
      <Key>CONF_FILE</Key>
      <Default>wowchat.conf</Default>
      <Description>Configuration file to use.</Description>
    </Variable>
  </Env>
  <Config>
	<Volume>
	<Host>/mnt/user/appdata/wowchat-epoch/src/main/resources</Host>
	<Container>/app/config</Container>
	</Volume>
  </Config>
  <Startup>
    <Sleep>0</Sleep>
  </Startup>
</Container>
